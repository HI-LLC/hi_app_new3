 column=(type=long updatewhereclause=yes name=lesson_content_pair_ind dbname="lesson_content.pair_ind" )
 column=(type=char(80) updatewhereclause=yes name=method_lesson_type dbname="method.lesson_type" )
 retrieve="  SELECT ~"progress_data~".~"total_tries~",   
         ~"progress_data~".~"total_correct_answers~",   
         ~"progress_data~".~"end_date~",   
         ~"progress_data~".~"begin_date~",           
         ~"student~".~"last_name~" + ', ' + ~"student~".~"first_name~" student_name,   
			~"lesson~".~"description~" lesson_name,
			~"lesson~".~"description~" + convert(char(10), ~"lesson~".~"lesson_id~") lesson_key,
			~"teacher~".~"last_name~" + ', ' + ~"teacher~".~"first_name~" teacher_name,
			~"content~".~"description~" lesson_item ,
			~"progress_data~".~"progress_data_id~" progress_data_id,
			~"lesson~".~"lesson_id~",
			~"progress_data~".~"lesson_content_id~" as lesson_content_id,
			~"lesson_content~".~"sequence~" as sequence,
			~"lesson_content~".~"pair_ind~" as pair_ind,
			~"method~".~"description~" as lesson_type
    FROM ~"lesson~",   
         ~"lesson_content~",   
         ~"progress_data~",   
         ~"student~",   
         ~"teacher~" ,
			~"content~",
			~"method~"
   WHERE ( ~"lesson~".~"lesson_id~" = ~"lesson_content~".~"lesson_id~" ) and  
         ( ~"lesson_content~".~"lesson_content_id~" = ~"progress_data~".~"lesson_content_id~" ) and  
         ( ~"lesson~".~"student_id~" = ~"student~".~"student_id~" ) and  
         ( ~"lesson~".~"teacher_id~" = ~"teacher~".~"teacher_id~" ) and
         ( ~"lesson_content~".~"content_id~" = ~"content~".~"content_id~" ) and
			( ~"lesson~".~"method_id~" = ~"method~".~"method_id~" ) and  
         ( convert(char(10), ~"lesson~".~"student_id~") like :as_student_id ) and  
         ( convert(char(10), ~"lesson~".~"method_id~") like :as_method_id ) and  
   		( convert(char(10), ~"lesson~".~"lesson_id~") like :as_lesson_id ) and
         ( convert(char(10), ~"progress_data~".~"progress_data_id~") like :as_report_id )
" arguments=(("as_student_id", string),("as_method_id", string),("as_lesson_id", string),("as_report_id", string))  sort="student_name A method_lesson_type A lesson_key A progress_data_id A lesson_content_sequence A progress_data_lesson_content_id A " )
