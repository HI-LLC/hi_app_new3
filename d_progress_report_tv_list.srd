 column=(type=decimal(0) updatewhereclause=yes identity=yes name=student_id dbname="student.student_id" )
 retrieve="  SELECT ~"progress_data~".~"end_date~" end_date,   
         ~"progress_data~".~"begin_date~" begin_date,
			 dateformat( ~"progress_data~".~"begin_date~", 'mm/dd/yyyyy') + ' To ' + dateformat(~"progress_data~".~"end_date~", 'mm/dd/yyyyy') period,		                 
         ~"student~".~"last_name~" + ', ' + ~"student~".~"first_name~" student_name,   
			~"lesson~".~"description~" lesson_name,
			~"lesson~".~"description~" + convert(char(10), ~"lesson~".~"lesson_id~") lesson_key,
			~"progress_data~".~"progress_data_id~" progress_data_id,
			~"lesson~".~"lesson_id~" lesson_id,
			~"method~".~"method_id~" method_id,
			~"method~".~"description~" lesson_type,
			~"student~".~"student_id~" student_id
    FROM ~"lesson~",   
         ~"lesson_content~",   
         ~"progress_data~",   
         ~"student~",
			~"method~"
   WHERE ( ~"lesson~".~"lesson_id~" = ~"lesson_content~".~"lesson_id~" ) and  
         ( ~"lesson_content~".~"lesson_content_id~" = ~"progress_data~".~"lesson_content_id~" ) and  
         ( ~"lesson~".~"student_id~" = ~"student~".~"student_id~" ) and
         ( ~"lesson~".~"method_id~" = ~"method~".~"method_id~" )
"  sort="student_name A lesson_type A lesson_name A begin_date A end_date A " )
